defaults:
  db:
    location: localhost
    name: army_ant
    type: mongo
  eval:
    metrics:
      favorite: [GMAP, MAP, NDCG@10, P@10]
    location: /opt/army-ant/eval
  service:
    ner:
      entity_list: /opt/army-ant/gazetteers/all.txt
  depend:
    stanford-ner: /opt/stanford-ner-2015-12-09
  jvm:
    memory: 5120
    other_args: -XX:+UseConcMarkSweepGC

engines:
  gow-wikipedia:
    name: Wikipedia Data - Graph of Word
    message: >-
      Don't forget to launch Gremlin Server with the "gow_wikipedia" graph
      configured (e.g., "bin/gremlin-server.sh gremlin-server-neo4j.yaml").
    db:
      name: wikipedia_data
    index:
      type: gow
      location: localhost:8182/gow_wikipedia

  goe-wikipedia_data:
    name: Wikipedia Data - Graph of Entity
    message: >-
      Don't forget to launch Gremlin Server with the "goe_wikipedia" graph
      configured (e.g., "bin/gremlin-server.sh gremlin-server-neo4j.yaml").
    db:
      name: wikipedia_data
    index:
      type: goe
      location: localhost:8182/goe_wikipedia

  gow-trec2017:
    name: TREC 2017 - Graph of Word
    message: >-
      Don't forget to launch Gremlin Server with the "gow_trec2017" graph configured
      (e.g., "gremlin-server-neo4j.yaml").
    db:
      name: aa_ssoar
    index:
      type: gow_csv
      location: localhost:8182/gow_trec2017

  goe-trec2017:
    name: TREC 2017 - Graph of Entity
    message: >-
      Don't forget to launch Gremlin Server with the "goe_trec2017" graph configured
      (e.g., "gremlin-server-neo4j.yaml").
    db:
      name: aa_ssoar
    index:
      type: goe_csv
      location: localhost:8182/goe_trec2017

  lucene-wapo:
    name: WaPo - Lucene
    db:
      name: aa_wapo
    index:
      type: lucene
      location: /opt/army-ant/indexes/wapo/lucene
    ranking:
      default:
        id: tf_idf
      functions:
        tf_idf:
          name: TF-IDF
        bm25:
          name: BM25
          params:
            k1: [1, 1.2, 1.8]
            b: [0.5, 0.75, 1]
        dfr:
          name: DFR
          params:
            BM: [BE, G, P, D, In, Ine, IF]
            AE: [L, B, Disabled]
            N: [H1, H2, H3, Z, Disabled]

  lucene-features-wapo:
    name: WaPo - Lucene Features
    db:
      name: aa_wapo
    index:
      type: lucene_features
      location: /opt/army-ant/indexes/wapo/lucene_features
    ranking:
      default:
        id: bm25
      functions:
        bm25:
          name: BM25
          params:
            k1: [1.2]
            b: [0.75]
            feature: ["indegree", "hits_authority", "pr", "fpr_out_indegree", "Disabled"]
            w: [1.8]
            k: [1.0]
            a: [0.6]

  lucene-wapo-sample:
    name: WaPo Sample - Lucene
    db:
      name: aa_wapo
    index:
      type: lucene
      location: /opt/army-ant/indexes/wapo-sample/lucene
    ranking:
      default:
        id: tf_idf
      functions:
        tf_idf:
          name: TF-IDF
        bm25:
          name: BM25
          params:
            k1: [1, 1.2, 1.8]
            b: [0.5, 0.75, 1]
        dfr:
          name: DFR
          params:
            BM: [BE, G, P, D, In, Ine, IF]
            AE: [L, B, Disabled]
            N: [H1, H2, H3, Z, Disabled]

  hgoe-wapo-sample:
    name: WaPo Sample - Hypergraph-of-Entity
    db:
      name: aa_wapo
    index:
      type: hgoe
      location: /opt/army-ant/indexes/wapo-sample/hgoe
      preload: false
    ranking:
      default:
        id: random_walk
        params:
          l: 2
          r: 100
      functions:
        jaccard:
          name: Jaccard Score
        random_walk:
          name: Random Walk Score
          params:
            l: [1, 2, 3, 4, 5, 6]
            r: [10, 100, 250, 500, 750, 1000, 10000, 100000]

  hgoe-doc_profile-wapo-sample:
